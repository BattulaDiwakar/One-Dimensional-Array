import java.util.Scanner;
public class Pos
{
    public static void main(String[] args)
    {Scanner sc=new Scanner(System.in);
    int a[]=new int[100];
    int n=sc.nextInt();
    for(int i=0;i<n;i++)
    {
        a[i]=sc.nextInt();
    }
     for(int i=0;i<n;i++)
    {
        if(a[i]==i*i)
        {
            System.out.print(a[i]);
        }
    }
    
        
    }
}
