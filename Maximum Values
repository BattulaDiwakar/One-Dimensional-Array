import java.util.Scanner;
public class Max
{
    public static void main(String[] args)
    {Scanner sc=new Scanner(System.in);
    int a[]=new int[10];
    int n=sc.nextInt();
    for(int i=0;i<n;i++)
    {
        a[i]=sc.nextInt();
    }
    int max=a[0];
    for(int i=0;i<n;i++)
    {
        if(a[i]>max)
        {   max=a[i];
            
        }
    }System.out.print(max);
       
    }
}
