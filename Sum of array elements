import java.util.Scanner;
public class Sumarray
{
    public static void main(String[] args)
    {
         int b=0;
         int sum=0;
         Scanner sc=new Scanner(System.in);
         int a[]=new int[100];
    int n=sc.nextInt();
  
    for(int i=0;i<n;i++)
    {
        a[i]=sc.nextInt();
    }
     for(int i=0;i<n;i++)
     { 
         
        sum=a[i]+b;
        b=sum;
        
         
     }
     System.out.println(""+sum);
       
    }
}
