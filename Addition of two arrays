import java.util.Scanner;
public class Add
{
    public static void main(String[] args)
    {Scanner sc=new Scanner(System.in);
    int n=sc.nextInt();
    int a[]=new int[100];
    int b[]=new int[100];
    int c[]=new int[100];
    for(int i=0;i<n;i++)
    {
        a[i]=sc.nextInt();
    }
       for(int i=0;i<n;i++)
    {
        b[i]=sc.nextInt();
    }
     for(int i=0;i<n;i++)
{
    c[i]=a[i]+b[i];
}
for(int i=0;i<n;i++)
{
    System.out.print(c[i]+" ");
}
    
    }
        
    
}
